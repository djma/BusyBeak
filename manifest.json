{
  "manifest_version": 2,
  "name": "The Receipts",
  "version": "1.0",

  "description": "Remember what you see. Always bring receipts.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "permissions": [
    "https://api.openai.com/*",
    "https://*.pinecone.io/*"
  ],

  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "js": ["dist/content_scripts.js"]
    }
  ]
}